lex:Del1029\/Artigo120\/AlineaPracas    lex:temItem    lex:Del1029\/Artigo120\/AlineaPracas\/ItemTerceiro-Sargento .
lex:Del1029\/Artigo120\/AlineaPracas\/ItemTerceiro-Sargento    lex:temConteudo    "- Terceiro-Sargento" .
lex:Del1029\/Artigo120\/AlineaPracas\/ItemSubtenente    lex:ehUm    lex:Item .
lex:Del1029\/Artigo120\/AlineaPracas    lex:temItem    lex:Del1029\/Artigo120\/AlineaPracas\/ItemSubtenente .
lex:Del1029\/Artigo120\/AlineaPracas\/ItemSubtenente    lex:temConteudo    "- Subtenente" .
lex:Del1029\/Artigo120\/AlineaPracas\/ItemPrimeiro-Sargento    lex:ehUm    lex:Item .
lex:Del1029\/Artigo120\/AlineaPracas    lex:temItem    lex:Del1029\/Artigo120\/AlineaPracas\/ItemPrimeiro-Sargento .
lex:Del1029\/Artigo120\/AlineaPracas\/ItemPrimeiro-Sargento    lex:temConteudo    "- Primeiro-Sargento" .
lex:Del1029\/Artigo120\/AlineaPracas\/ItemSegundo-Sargento    lex:ehUm    lex:Item .
lex:Del1029\/Artigo120\/AlineaPracas    lex:temItem    lex:Del1029\/Artigo120\/AlineaPracas\/ItemSegundo-Sargento .
lex:Del1029\/Artigo120\/AlineaPracas\/ItemSegundo-Sargento    lex:temConteudo    "- Segundo-Sargento" .
lex:Del1029\/Artigo120\/AlineaPracas\/ItemTerceiro-Sargento    lex:ehUm    lex:Item .
lex:Del1029\/Artigo120\/AlineaPracas    lex:temItem    lex:Del1029\/Artigo120\/AlineaPracas\/ItemTerceiro-Sargento .
lex:Del1029\/Artigo120\/AlineaPracas\/ItemTerceiro-Sargento    lex:temConteudo    "- Terceiro-Sargento" .
lex:Del1029\/Artigo120\/AlineaPracas\/ItemSuboficial    lex:ehUm    lex:Item .
lex:Del1029\/Artigo120\/AlineaPracas    lex:temItem    lex:Del1029\/Artigo120\/AlineaPracas\/ItemSuboficial .
lex:Del1029\/Artigo120\/AlineaPracas\/ItemSuboficial    lex:temConteudo    "- Suboficial" .
lex:Del1029\/Artigo120\/AlineaPracas\/ItemPrimeiro-Sargento    lex:ehUm    lex:Item .
lex:Del1029\/Artigo120\/AlineaPracas    lex:temItem    lex:Del1029\/Artigo120\/AlineaPracas\/ItemPrimeiro-Sargento .
lex:Del1029\/Artigo120\/AlineaPracas\/ItemPrimeiro-Sargento    lex:temConteudo    "- Primeiro-Sargento" .
lex:Del1029\/Artigo120\/AlineaPracas\/ItemSegundo-Sargento    lex:ehUm    lex:Item .
lex:Del1029\/Artigo120\/AlineaPracas    lex:temItem    lex:Del1029\/Artigo120\/AlineaPracas\/ItemSegundo-Sargento .
lex:Del1029\/Artigo120\/AlineaPracas\/ItemSegundo-Sargento    lex:temConteudo    "- Segundo-Sargento" .
lex:Del1029\/Artigo120\/AlineaPracas\/ItemTerceiro-Sargento    lex:ehUm    lex:Item .
lex:Del1029\/Artigo120\/AlineaPracas    lex:temItem    lex:Del1029\/Artigo120\/AlineaPracas\/ItemTerceiro-Sargento .
lex:Del1029\/Artigo120\/AlineaPracas\/ItemTerceiro-Sargento    lex:temConteudo    "- Terceiro-Sargento" .
lex:Del1029\/Artigo120\/AlineaPracas\/ItemSuboficiais    lex:ehUm    lex:Item .
lex:Del1029\/Artigo120\/AlineaPracas    lex:temItem    lex:Del1029\/Artigo120\/AlineaPracas\/ItemSuboficiais .
lex:Del1029\/Artigo120\/AlineaPracas\/ItemSuboficiais    lex:temConteudo    "- Suboficiais, Subtenentes e Sargentos" .
lex:Del1029\/Artigo120\/AlineaPracas\/ItemCabo    lex:ehUm    lex:Item .
